# Environment Variables Template for CutClub - Académie Précision
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Stripe Configuration
STRIPE_PUBLISHABLE_KEY_DEV=pk_test_YOUR_KEY_HERE
STRIPE_PUBLISHABLE_KEY_STAGING=pk_test_YOUR_STAGING_KEY_HERE
STRIPE_PUBLISHABLE_KEY=pk_live_YOUR_PRODUCTION_KEY_HERE

# API Configuration
API_BASE_URL_DEV=http://localhost:3000/api
API_BASE_URL_STAGING=https://staging-api.academie-precision.com/api
API_BASE_URL=https://api.academie-precision.com/api

# Supabase Configuration
SUPABASE_URL_DEV=https://YOUR_PROJECT.supabase.co
SUPABASE_ANON_KEY_DEV=YOUR_ANON_KEY_HERE
SUPABASE_URL_STAGING=https://YOUR_STAGING_PROJECT.supabase.co
SUPABASE_ANON_KEY_STAGING=YOUR_STAGING_ANON_KEY_HERE
SUPABASE_URL=https://YOUR_PRODUCTION_PROJECT.supabase.co
SUPABASE_ANON_KEY=YOUR_PRODUCTION_ANON_KEY_HERE

# Security Configuration
SESSION_SECRET=YOUR_SECRET_HERE
ENABLE_SECURITY_HEADERS=true

# EAS Configuration (for Expo builds)
EAS_PROJECT_ID=YOUR_EAS_PROJECT_ID_HERE

# Release Channel
RELEASE_CHANNEL=default